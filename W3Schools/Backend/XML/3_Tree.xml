<!-- Section 3: Tree -->

<!-- XML uses a tree to structure its data. -->

<!-- A prolog defines the XML version and the character encoding: -->
<?xml version="1.0" encoding="UTF-8"?> 

<!-- The next line is the root element of the document: -->
<root>
  <child>
    <subchild>.....</subchild>
  </child>
</root>
